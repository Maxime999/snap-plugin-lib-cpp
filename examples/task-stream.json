{
	"version":1,
	"schedule":{
		"type":"streaming"
	},
	"max-failures":10,
	"workflow":{
		"collect":{
			"metrics":{
				"/intel/cpp/mock/rando_stream/boolean":{},
				"/intel/cpp/mock/rando_stream/int64":{}
			},
			"config":{
				"/intel/cpp/mock/rando_stream":{
					"MaxCollectDuration": 10,
					"MaxMetricsBuffer": 4
				}
			},
			"process":[
				{
					"plugin_name":"graffiti",
					"config":{
						"tags":"one,two,three"
					},
					"publish":[
						{
							"plugin_name":"log",
							"config":{
								"path":"/tmp/log_pub_stream.log"
							}
						}
					]
				}
			]
		}
	}
}
