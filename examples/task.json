{
	"version":1,
	"schedule":{
		"type":"simple",
		"interval":"1s"
	},
	"max-failures":10,
	"workflow":{
		"collect":{
			"metrics":{
				"/intel/cpp/mock/rando/int32":{},
 				"/intel/cpp/mock/rando/string":{},
 				"/intel/cpp/mock/rando/int64":{},
 				"/intel/cpp/mock/rando/boolean":{},
 				"/intel/cpp/mock/dynamic/*/string":{}
			},
			"config":{
				"/intel/cpp/mock/randomnumber/one":{
					"password":"silly goose"
				}
			},
			"process":[
				{
					"plugin_name":"graffiti",
					"config":{
						"tags":"one,two,three"
					},
					"publish":[
						{
							"plugin_name":"log",
							"config":{
								"path":"/tmp/log_pub.log"
							}
						}
					]
				}
			]
		}
	}
}
